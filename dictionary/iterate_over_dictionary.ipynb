   "source": [
    "# Write a Python program to iterate over dictionaries using for loops.\r\n",
    "dic= {1: \"le\",\r\n",
    "2: \"Thị\",\r\n",
    "3: \"Quỳnh\",\r\n",
    "4: \"Trang\"}\r\n",
    "\r\n",
    "#solution 1\r\n",
    "for key, value in dic.items():\r\n",
    "    print(key, value)\r\n",
    "\r\n",
    "#Solution 2\r\n",
    "for key in dic:\r\n",
    "    print(key,dic[key])"
   ]
