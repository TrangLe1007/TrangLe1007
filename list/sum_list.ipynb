   ],
   "source": [
    "def sum_list(a):\r\n",
    "    sum=0\r\n",
    "    for i in range (len(a)):\r\n",
    "        sum=sum+a[i]\r\n",
    "    return sum\r\n",
    "sum_list([1,2,3])"
   ]
  }
